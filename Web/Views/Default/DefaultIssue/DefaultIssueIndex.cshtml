@model IEnumerable<SolutionNorSolutionPort.BusinessLogicLayer.CrudeDefaultIssueContract>

@{
    ViewBag.Title = "Index Default Issue";
}

<h2>Index for Default Issue</h2>

<p>
    @Html.ActionLink("Create New", "DefaultIssueCreate", "DefaultIssue" )
</p>

<table class="table">
    <tr>
        <th>Default Issue Type</th>
        <th>Default Issue Status</th>
        <th>Issue Name</th>
        <th>Issue Description</th>
        <th>Steps To Reproduce</th>
        <th>Link</th>
        <th>Date Time</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.DefaultIssueTypeRcd) </td>
            <td>@Html.DisplayFor(modelItem => item.DefaultIssueStatusRcd) </td>
            <td>@Html.DisplayFor(modelItem => item.IssueName) </td>
            <td>@Html.DisplayFor(modelItem => item.IssueDescription) </td>
            <td>@Html.DisplayFor(modelItem => item.StepsToReproduce) </td>
            <td>@Html.DisplayFor(modelItem => item.Link) </td>
            <td>@Html.DisplayFor(modelItem => item.DateTime) </td>
            <td>
                @Html.ActionLink("Edit", "DefaultIssueEdit", "DefaultIssue", null, new {defaultIssueId = item.DefaultIssueId})
            </td>
        </tr>
    }

</table>
