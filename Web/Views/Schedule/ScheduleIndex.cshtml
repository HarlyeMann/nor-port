@model IEnumerable<SolutionNorSolutionPort.BusinessLogicLayer.CrudeFlightScheduleContract>

@{
    ViewBag.Title = "Index Schedule";
}

<h2>Index for Schedule</h2>

<p>
    @Html.ActionLink("Create New", "ScheduleCreate")
</p>

<table class="table">
    <tr>
        <th>Aircraft Type</th>
        <th>Comment</th>
        <th>From Date Time</th>
        <th>Until Date Time</th>
        <th>Date Time</th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.AircraftTypeRcd) </td>
            <td>@Html.DisplayFor(modelItem => item.Comment) </td>
            <td>@Html.DisplayFor(modelItem => item.FromDateTime) </td>
            <td>@Html.DisplayFor(modelItem => item.UntilDateTime) </td>
            <td>@Html.DisplayFor(modelItem => item.DateTime) </td>
            <td>
                @Html.ActionLink("Edit", "ScheduleEdit", new { flightScheduleId = item.FlightScheduleId })
            </td>
        </tr>
    }

</table>
