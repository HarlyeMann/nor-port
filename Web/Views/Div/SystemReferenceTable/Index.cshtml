@model IEnumerable<SolutionNorSolutionPort.BusinessLogicLayer.CrudeDefaultSystemReferenceTableContract>

@{
    ViewBag.Title = "Index";
}

<table class="table">
    <tr>
        <th>
            Display Name
        </th>
        <th>
            Changed Date Time
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DefaultSystemReferenceDisplayName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateTime)
            </td>
            <td>
                @if (!item.DefaultSystemReferenceTableName.IsEmpty()) {
                    @Html.ActionLink(   "Edit",
                                        "Crude" + item.DefaultSystemReferenceTableName + "Index",
                                        "Crude" + item.DefaultSystemReferenceTableName
                                        ); 
                }
            </td>
        </tr>
    }

</table>
